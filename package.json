  "bugs": {
    "url": "https://github.com/abr-ya/html-outdoors/issues"
  },
  "homepage": "https://github.com/abr-ya/html-outdoors#readme",
  "devDependencies": {
    "concurrently": "^7.3.0",
    "live-server": "^1.2.2",
    "node-sass": "^7.0.1"
  }
}
