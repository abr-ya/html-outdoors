{
  "name": "html-outdoors",
  "version": "1.0.0",
  "description": "Outdoors landing project: html, sass, js",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass ./src/scss/main.scss ./docs/css/style.css -w",
    "server": "live-server --open=./docs/ --watch=./docs/",
    "start": "concurrently --names \"SASS,SERVER\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run sass\" \"npm run server\""
  },
